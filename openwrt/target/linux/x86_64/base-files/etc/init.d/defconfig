START=05

start() {
	. /lib/soekris.sh

	local board=astor2

	for f in $( ls /etc/defconfig/$board ); do
		if [ ! -e /etc/config/$f ]; then
			cp /etc/defconfig/$board/$f /etc/config/
		fi
	done
}
